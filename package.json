    "clsx": "^2.0.0",
    "cors": "^2.8.5",
    "date-fns": "^3.0.6",
    "ethers": "^6.9.0",
    "express": "^4.18.2",
    "framer-motion": "^10.16.16",
    "lucide-react": "^0.302.0",
    "next": "^14.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^2.2.0",
