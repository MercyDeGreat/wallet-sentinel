{
  "name": "securnex",
  "version": "1.0.0",
  "description": "Securnex - Blockchain security analysis platform for wallet compromise detection and recovery",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:protected": "next build && node scripts/obfuscate.js",
